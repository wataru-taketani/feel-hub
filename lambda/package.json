{
  "name": "feel-hub-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda functions for Feel Hub",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "npm run build && serverless deploy",
    "deploy:dev": "npm run build && serverless deploy --stage dev",
    "deploy:prod": "npm run build && serverless deploy --stage prod",
    "invoke:scrape": "serverless invoke local --function scrapeLesson",
    "logs": "serverless logs -f scrapeLesson --tail"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/node": "^20",
    "serverless": "^4.0.0",
    "serverless-offline": "^14.0.0",
    "typescript": "^5"
  }
}
